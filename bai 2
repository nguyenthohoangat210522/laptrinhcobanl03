#include <stdio.h>
int main()
{
    int a,b,c;
    printf("moi ban nhap ngay hom nay : ");
    scanf("%d",&a);
     printf("moi ban nhap thang hom nay : ");
    scanf("%d",&b);
     printf("moi ban nhap nam hom nay : ");
    scanf("%d",&c);
    printf("hom nay la %d/%d/%d",a,b,c);


    return 0;
}
