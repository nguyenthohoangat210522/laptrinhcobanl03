#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a=400,b=200;
    char n;
    scanf("%c",&n);
    switch(n){
    case'+':
        printf(" tong la %d",a+b);
        break;
    case'-':
        printf(" hieu la %d",a-b);
        break;
    case'*':
        printf(" tich la %d",a*b);
        break;
    case'/':
        printf(" hieu la %d",a/b);
        break;
    default:
        printf("du lieu khong hop le");


}
    return 0;
}
